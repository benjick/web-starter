{
  "name": "web-starter",
  "version": "0.1.0",
  "description": "Web starter kit",
  "main": "src/main.js",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "immutable": "^3.8.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.2.0",
    "ava": "^0.15.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.12",
    "node-libs-browser": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benjick/web-starter.git"
  },
  "scripts": {
    "test": "ava --tap | tap-spec",
    "start:webpack": "webpack-dev-server --hot",
    "start": "concurrently 'npm run start:webpack' 'npm run lint:watch'",
    "lint:watch": "npm run lint -- -w --fix",
    "lint": "esw src"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "babel": {
    "presets": ["react", "es2015", "stage-0"]
  },
  "ava": {
    "require": ["babel-register"],
    "babel": "inherit"
  }
}
